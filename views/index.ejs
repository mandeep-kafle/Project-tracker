<!--include header-->

<%- include('include/_header')%>


<main id="site-main">
    <div class="container">
        <div class="box-nav d-flex-justify-between">
            <a href="/add_project" class="border-shadow">
                <span class="text-gradient">ADD PROJECT <i class="fas fa-folder-plus"></i> </span>
            </a>
        </div>
        <!-- form handling -->
        <form action="/" method="POST">
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        <th>ID</th>
                        <th>Date</th>
                        <th>Name</th>
                        <th>Repository</th>
                        <th>Web url</th>
                        <th>State</th>
                        <th>Issues</th>
                        <th>Action</th>

                </thead>
                </tr>
                <tbody>
                    <%- include('include/_show')%>
                </tbody>

            </table>
        </form>

    </div>
</main>

<!--/include header-->

<%- include('include/_footer')%>